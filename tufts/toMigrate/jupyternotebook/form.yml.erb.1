---

cluster: "pax"

attributes:
  conda_extensions: "0"

  extra_jupyter_args: ""


<%= File.read("/etc/ood/config/apps/dashboard/batch_connect/partials/small/num_cores.yml").indent(2) %>

<%= File.read("/etc/ood/config/apps/dashboard/batch_connect/partials/small/num_memory.yml").indent(2) %>

  version:
    widget: select
    label: "Jupyter, R, Python Version"
    help: "This defines the version of Jupter you want to load."
    options:
      - [ "Jupyter 4.4.0 Python 2.7.14", "jupyter/1.0" ]
      - [ "Jupyter 4.4.0 Python 3.7.1", "jupyter/3.0" ]
      - [ "Jupyter 6.0.3 Python 3.8.3", "jupyter/2020.07" ]

<%= File.read("/etc/ood/config/apps/dashboard/batch_connect/partials/reservation.yml").indent(2) %>

form:
  - conda_extensions
  - extra_jupyter_args
#  - bc_account
#  - bc_queue
  - bc_num_hours
  - num_cores
  - num_memory
  - version
  - reservation
#  - bc_num_slots
#  - bc_email_on_started
